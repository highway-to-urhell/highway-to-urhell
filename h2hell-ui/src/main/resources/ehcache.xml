<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
         name="CM1"
         updateCheck="false"
         maxBytesLocalHeap="16M">

    <!--
      This is a default configuration, it is re-configured by the CacheConfiguration Spring Bean, using the
      properties from the resources/config/*.yml files.
    -->

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
            eternal="false"
            overflowToDisk="false"
            />

    <cache name="com.highway2urhell.domain.User"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Authority"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.User.authorities"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.PersistentToken"
           timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.User.persistentTokens"
           timeToLiveSeconds="3600">
    </cache>
    
    <cache name="com.highway2urhell.domain.Application"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Application.analyses"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Application.userPermissions"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.EntryPointCall"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Event"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.EntryPointPerf"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Analysis"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Analysis.events"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Analysis.entryPoints"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.EntryPoint"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.EntryPoint.perfs"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.EntryPoint.calls"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Team"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Team.members"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.Team.userPermissions"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.UserPermission"
        timeToLiveSeconds="3600">
    </cache>

    <cache name="com.highway2urhell.domain.UserPermission.projects"
        timeToLiveSeconds="3600">
    </cache>

    <!-- jhipster-needle-ehcache-add-entry -->
</ehcache>
