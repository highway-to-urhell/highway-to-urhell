<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="com.highway2urhell.domain.User" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Authority" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.User.authorities" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.PersistentToken" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Analysis" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Analysis.events" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Analysis.entryPoints" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Application" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Application.analyses" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Application.userPermissions" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.EntryPoint" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.EntryPoint.perfs" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.EntryPoint.calls" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.EntryPointCall" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.EntryPointPerf" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Event" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Team" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Team.members" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.Team.userPermissions" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.UserPermission" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.UserPermission.projects" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.EntryPoint.metrics" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.EntryPoint.logs" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.EntryPointParameters" uses-template="simple"/>

    <cache alias="com.highway2urhell.domain.MetricsTimer" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
